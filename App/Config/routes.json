{
    "baseUri": "/desart",
    "routes": {
        "/ajax": {
            "/login": [
              "POST",
              ["App\\Controller\\Auth", "login"]
            ],
            "/register": [
              "POST",
              ["App\\Controller\\Auth", "register"]
            ],
            "/topic/add/{num}": [
                "GET",
                ["App\\Controller\\Forum\\Topic", "add"]
            ],
            "/post/add/{num}": [
                "GET",
                ["App\\Controller\\Forum\\Post", "add"]
            ]
        }
    }
}
