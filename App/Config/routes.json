{
    "baseUri": "/desart",
    "routes": {
        "/": ["GET", ["App\\Presenter\\Home", "index"]],
        "/profil/{slug}": ["GET", ["App\\Presenter\\User", "view"]],
        "/clanok/{slug}": ["GET", ["App\\Presenter\\Article", "view"]],
        "/clanky": ["GET", ["App\\Presenter\\Article", "overview"]],
        "/forum/novatema/{category}": ["GET", ["App\\Presenter\\Forum\\Topic", "create"]],
        "/tema/{slug}": [
            "GET",
            ["App\\Presenter\\Forum\\Topic", "view"]
        ],
        "/tema/?{slug}/strana/?{page}": [
            "GET",
            ["App\\Presenter\\Forum\\Topic", "view"]
        ],
        "/forum/kategoria/?{slug}/strana/?{page}": [
            "GET",
            ["App\\Presenter\\Forum\\Topic", "overviewCategory"]
        ],
        "/forum/kategoria/?{category}": [
            "GET",
            ["App\\Presenter\\Forum\\Topic", "overviewCategory"]
        ],
        "/forum": [
            "GET",
            ["App\\Presenter\\Forum\\Topic", "overview"]
        ],
        "/api": {
            "/topic/add/{num}": [
                "GET",
                ["App\\Controller\\Forum\\Topic", "add"]
            ],
            "/post/add/{num}": [
                "GET",
                ["App\\Controller\\Forum\\Post", "add"]
            ]
        }
    }
}
